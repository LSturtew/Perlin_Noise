   "source": [
    "from scipy.stats import norm\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def brownian(delta, dt, x, n):\n",
    "    for k in range(n):\n",
    "        x = x + norm.rvs(scale=delta**2*dt)\n",
    "        yield x\n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "x = [v for v in brownian(0.25, .1, 0.0, 50)]\n",
    "plt.plot(x)\n",
    "plt.show()"
   ]
